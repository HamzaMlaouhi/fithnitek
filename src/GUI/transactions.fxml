      <Button fx:id="btnPrintBill" layoutX="240.0" layoutY="340.0" mnemonicParsing="false" onAction="#PrintBill" prefHeight="29.0" prefWidth="126.0" style="-fx-background-color: #7B3F9F;" text="Print Bill" textFill="WHITE" />
