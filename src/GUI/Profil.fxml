<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ProfilController">
   <children>
      <ImageView fx:id="ProfImage" fitHeight="109.0" fitWidth="113.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="txtFull_name" layoutX="134.0" layoutY="13.0" prefHeight="29.0" prefWidth="113.0" text="Label">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <Label fx:id="txtEmail" layoutX="134.0" layoutY="55.0" prefHeight="29.0" prefWidth="113.0" text="Label">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <Label fx:id="txtTelNum" layoutX="134.0" layoutY="95.0" prefHeight="29.0" prefWidth="113.0" text="Label">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <Button fx:id="btnSetting" layoutX="14.0" layoutY="130.0" mnemonicParsing="false" onAction="#SettingAction" prefHeight="37.0" prefWidth="119.0" style="-fx-background-color: #0598ff;" text="Setting" textFill="WHITE" />
      <Button fx:id="btnLogOut" layoutX="14.0" layoutY="182.0" mnemonicParsing="false" onAction="#LogOutAction" prefHeight="37.0" prefWidth="119.0" style="-fx-background-color: #0598ff;" text="Log Out" textFill="WHITE" />
      <Button fx:id="livBtn" layoutX="467.0" layoutY="164.0" mnemonicParsing="false" onAction="#goToLivraison" prefHeight="37.0" prefWidth="119.0" style="-fx-background-color: #0598ff;" text="Home Page">
         <textFill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="WHITE" />
                  <Stop color="WHITE" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill></Button>
      <Button fx:id="btnMyPackages" layoutX="467.0" layoutY="24.0" mnemonicParsing="false" onAction="#goToMyPackages" prefHeight="37.0" prefWidth="119.0" style="-fx-background-color: #0598ff;" text="My Packages">
         <textFill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="WHITE" />
                  <Stop color="WHITE" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
      </Button>
      <Button fx:id="btnMyItems" layoutX="467.0" layoutY="66.0" mnemonicParsing="false" onAction="#goToMyItems" prefHeight="37.0" prefWidth="119.0" style="-fx-background-color: #0598ff;" text="My Items">
         <textFill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="WHITE" />
                  <Stop color="WHITE" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
      </Button>
      <Button fx:id="btnMyTrips" layoutX="467.0" layoutY="112.0" mnemonicParsing="false" onAction="#goToMyTrips" prefHeight="37.0" prefWidth="119.0" style="-fx-background-color: #0598ff;" text="My Trips">
         <textFill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="WHITE" />
                  <Stop color="WHITE" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
      </Button>
   </children>
</AnchorPane>
