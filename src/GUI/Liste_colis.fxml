<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Liste_colisController">
   <children>
      <TableView fx:id="listcolis" layoutX="45.0" layoutY="20.0" prefHeight="257.0" prefWidth="511.0">
        <columns>
          <TableColumn fx:id="from" onEditCommit="#changeDepart" prefWidth="75.0" text="From" />
          <TableColumn fx:id="to" onEditCommit="#changeDestination" prefWidth="75.0" text="To" />
            <TableColumn fx:id="date" onEditCommit="#changeDate" prefWidth="129.0" text="Limit Date" />
            <TableColumn fx:id="name" onEditCommit="#changeLabel" prefWidth="121.0" text="Shipment Name" />
            <TableColumn fx:id="note" onEditCommit="#changeNote" prefWidth="110.0" text="Note" />
        </columns>
      </TableView>
      <Button fx:id="btnadd" layoutX="81.0" layoutY="296.0" mnemonicParsing="false" onAction="#backToColis" text="Ajouter un Colis" />
      <Button fx:id="btnModifier" layoutX="248.0" layoutY="296.0" mnemonicParsing="false" onAction="#updateColis" text="Modifier" />
      <Button fx:id="btnSupprimer" layoutX="363.0" layoutY="296.0" mnemonicParsing="false" onAction="#DeleteColis" text="Supprimer" />
      <Button fx:id="btnSeeItems" layoutX="477.0" layoutY="296.0" mnemonicParsing="false" onAction="#seeItems" text="See items" />
   </children>
</AnchorPane>
